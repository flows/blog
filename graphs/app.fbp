title: Blog Generator

provider ./graphs/{ns}/{name}.fbp as app

App(app:rh/site)

# target directory
'site' -> dir App

# @pattern:App Pattern - Matching pattern for markdown files
'./site.md/*.md' -> pattern App

# @siteTemplate:App Site Template - The handlebars site template to use
'./template/site.hb' -> siteTemplate App

# The handlebars layout template to use
'./template/layout.hb' -> layoutTemplate App
